{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "semi": ["error", "always"],
    "no-undef": "error",
    "no-unused-vars": "error",
    "func-style": ["error", "declaration"]
  }
}
